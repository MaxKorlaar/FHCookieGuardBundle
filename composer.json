{
  "name": "freshheads/cookie-guard-bundle",
  "type": "symfony-bundle",
  "description": "The FHCookieGuardBundle can be used to activate (external) scripts when cookies are accepted by the client",
  "require": {
    "php": "^5.6 || ^7.0",
    "symfony/config": "^3.0 || ^4.0",
    "symfony/dependency-injection": "^3.0 || ^4.0",
    "symfony/http-kernel": "^3.0 || ^4.0",
    "twig/extensions": "^1.3"
  },
  "autoload": {
    "psr-4": { "FH\\Bundle\\CookieGuardBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "3.x-dev"
    }
  }
}
